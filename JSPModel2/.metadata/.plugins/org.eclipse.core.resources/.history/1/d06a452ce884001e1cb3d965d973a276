package com.kwak.nov163.jstlf;

import java.util.Date;

public class Snack {
	private String name;
	private int pirce;
	private Date exp;
	
	public Snack() {
		// TODO Auto-generated constructor stub
	}

	public Snack(String name, int pirce, Date exp) {
		super();
		this.name = name;
		this.pirce = pirce;
		this.exp = exp;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getPirce() {
		return pirce;
	}

	public void setPirce(int pirce) {
		this.pirce = pirce;
	}

	public Date getExp() {
		return exp;
	}

	public void setExp(Date exp) {
		this.exp = exp;
	}
	
	
}
