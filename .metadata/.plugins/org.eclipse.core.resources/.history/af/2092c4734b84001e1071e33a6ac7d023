<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    <%--자동완성 사용하기, prefix="c"는 정해진거라 바꿀수업슴 --%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>    
    
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>output.jsp</title>
</head>
<body>
	<h1>${param.n }</h1>
	<%--조건문 --%>
	<c:if test="${param.n % 2 == 0 }"><h3>짝수</h3></c:if>
</body>
</html>