<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kwak.nov301.student.StudentMapper">
	<insert id="regStudent" parameterType="com.kwak.nov301.student.Student">
		insert into nov29_student values(
			nov29_student_seq.nextval, #{s_name}, #{s_nickname})
	</insert>
	
	<select id="getAllStudent" resultType="com.kwak.nov301.student.Student">
		select*from NOV29_STUDENT order by s_no	
	</select>
</mapper>